<p id="notice"><%= notice %></p>

<h1>Logs</h1>

<table>
  <thead>
    <tr>
      <th>Fed</th>
      <th>Defecation</th>
      <th>Cleaned</th>
      <th>Length</th>
      <th>Weight</th>
      <th>Shed</th>
      <th>Fed</th>
      <th>Prey item</th>
      <th>Refused</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.fed %></td>
        <td><%= log.defecation %></td>
        <td><%= log.cleaned %></td>
        <td><%= log.length %></td>
        <td><%= log.weight %></td>
        <td><%= log.shed %></td>
        <td><%= log.fed %></td>
        <td><%= log.prey_item %></td>
        <td><%= log.refused %></td>
        <td><%= link_to 'Show', animal_log_path(@animal, log) %></td>
        <td><%= link_to 'Edit', edit_animal_log_path(@animal, log) %></td>
        <td><%= link_to 'Destroy', animal_log_path(@animal, log), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log', new_animal_log_path %>
